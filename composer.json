{
    "name":         "dwr/avatar-bundle",
    "description":  "Simply symfony2 bundle which creates avatars images using GD.",
    "keywords":     ["avatar", "avatar generator", "image"],
    "homepage":     "https://github.com/dariuszwrzesien/DwrAvatarBundle.git",
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors": [
        {
        "name":     "Dariusz Wrzesien",
        "email":    "dariuszwrzesien@gmail.com",
        "homepage": "https://github.com/dariuszwrzesien/DwrAvatarBundle"
        }
    ],
    "require": {
        "symfony/framework-bundle": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "symfony/twig-bundle": "2.*"
    },
    "autoload": {
        "psr-0": { "Dwr\\AvatarBundle": "" }
    },
    "target-dir": "Dwr/AvatarBundle"
}
